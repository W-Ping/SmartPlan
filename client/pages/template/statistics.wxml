<template name="statistics-plan">
  <view class='stat-plan-box' data-pno="{{item.plan_no}}" bindtap='navigatorStatPlan'>
    <view class='spb-day'>
      <text>{{item.start_time}}~{{item.end_time}}</text>
    </view>
    <view class='spb-split'></view>
    <view class='spb-declare'>
      <text>新增目标 <text class='plan-count addCount'>{{item.count_sum}}</text>个，完成目标<text  class='plan-count finishCount'>{{item.done_count}}</text>个</text>
    </view>
  </view>
  <!-- <view></view> -->
</template>